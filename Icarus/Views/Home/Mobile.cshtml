        font-size: 10px;
        font-weight:bold;
        margin:13px 0px 0px 0px;
    }
    #Arches {
        position:absolute;
    }

    #Natives{
        position:absolute;
    }
    .SmallIcons {
        margin: 0px 5px 0px 5px
    }
</style>
<script type="text/javascript">
    let myZoom = 0;
    let lineWidth = 6;
    let shim = 0;
    document.addEventListener('wheel', function(event){
        event.preventDefault();
        if (event.deltaY < 0) {
            myZoom = Math.min(120, (myZoom + 20));
        }
        else{
            myZoom = Math.max(0, (myZoom - 20));
        }
        drawFloaters();
    });
    let getHeight = ()=>{
        return document.documentElement.clientHeight;
        let D = document;
        //alert(D.body.scrollHeight + ", " + D.documentElement.scrollHeight + ", " + D.body.offsetHeight + ", " + D.documentElement.offsetHeight + ", " + D.body.clientHeight + ", " + D.documentElement.clientHeight  );
        //return Math.max(
        //    D.body.scrollHeight, D.documentElement.scrollHeight,
        //    D.body.offsetHeight, D.documentElement.offsetHeight,
        //    D.body.clientHeight, D.documentElement.clientHeight
        //);    
    };
    let getWidth = ()=>{
        let D=document;
    //alert(
    //        D.body.scrollWidth + ", " + D.documentElement.scrollWidth + ", " +
    //        D.body.offsetWidth + ", " + D.documentElement.offsetWidth + ", " +
    //        D.body.clientWidth + ", " + D.documentElement.clientWidth)

        return Math.min(
            D.body.scrollWidth, D.documentElement.scrollWidth,
            D.body.offsetWidth, D.documentElement.offsetWidth,
            D.body.clientWidth, D.documentElement.clientWidth
        );    
    }
    let drawFloaters = ()=>{
        let height = getHeight();
        let width = getWidth();

        let graphics = document.getElementsByClassName('Graphic');
        for(graphic of graphics){
            graphic.style.display="none"
        }
        let productImgs = document.getElementsByClassName('Product');
        if(myZoom >= -1){
            for(product of productImgs){
                product.style.display="";
                product.style.width = (width /6) + "px";
                product.style.height = (height/10) + "px";
            }
            if(myZoom > 0){
                let companyImgs = document.getElementsByClassName('Company');
                for(company of companyImgs)
                    company.style = "display:'' width:'' height:''";
                for(product of productImgs)
                    product.style = "display:'' width:'' height:''";
                let nativeImgs = document.getElementById('Natives')
                nativeImgs.style = "display:'' width:'' height:''; width:5%";
                if(myZoom > 20){
                    let legendImgs = document.getElementsByClassName('Legend')
                    for(legend of legendImgs)
                        legend.style = "display:'' width:'' height:''";
                    if(myZoom > 40){
                        let smallIcons = document.getElementsByClassName('SmallIcons');
                        for(smallIcon of smallIcons)
                            smallIcon.style = "display:'' width:'' height:''";
                        if(myZoom > 60){
                            let opsDepsImgs = document.getElementsByClassName('OpsDeps')
                            let languageImgs = document.getElementsByClassName('Language');
                            for(opsDep of opsDepsImgs)
                                opsDep.style = "display: ''; width:''; height:''";
                            for(language of languageImgs)
                                language.style = "display:'' width:'' height:''";
                            if(myZoom > 80){
                                let details = document.getElementsByClassName('Details')
                                for(detail of details){
                                    detail.style.display="";
                                    detail.style.width="";
                                    detail.style.height="";
                                }
                                if(myZoom > 100){
                                    let archs = document.getElementById('Arches');
                                        archs.style = "display:''; width:''; height:'';";
        }   }   }   }   }   }   }
        let native          = document.getElementById('native');
        let compileToNative = document.getElementById('compileToNative');
        let hybridWrapper   = document.getElementById('hybridWrapper');

        hybridWrapper.style.top    = (shim + 1) - (myZoom/20) + "px";
        hybridWrapper.style.left   = "2px";
        hybridWrapper.style.width  = (width - 40) + "px";
        hybridWrapper.style.height = (height - (16-(myZoom/20))) + "px";
        hybridWrapper.style.borderRadius = "20px";
        hybridWrapper.style.borderWidth = (1+(6 - (myZoom / 20))) + "px";

        compileToNative.style.top    = ((shim - 5)+ (height/4)) + "px";
        compileToNative.style.left   = (2 + (lineWidth*1.5)) + "px";
        compileToNative.style.width  = (((width/4)*3)-130) + "px";
        compileToNative.style.height = (height/2) + "px";
        compileToNative.style.borderRadius = "20px";
        compileToNative.style.borderWidth = (1+(6 - (myZoom / 20)))+ "px";

        native.style.top    = (shim + (((height/4) + 24) - (myZoom/20))) + "px";
        native.style.left   = (width/4) + "px";
        native.style.width  = ((width/2) - 150) + "px";
        native.style.height = ((height/4)-30) + "px";
        native.style.borderRadius = "20px";
        native.style.borderWidth = (1+(6 - (myZoom / 20))) + "px";

        let WKWebView = document.getElementById('ApacheWKWebView');
