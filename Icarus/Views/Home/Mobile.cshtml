        let XamarinIOS = document.getElementById('XamarinIOS');
        XamarinIOS.style.top    = (shim + (((height/4) + 55) - (myZoom/20))) + "px";
        XamarinIOS.style.left   = (shim + ((width/4))-26)  + "px";
        XamarinIOS.style.width  = (width/15)  + "px";
        XamarinIOS.style.height = (height/40) + "px";

        let nativeTitle          = document.getElementById('nativeTitle');
        let compileToNativeTitle = document.getElementById('compileToNativeTitle');
        let hybridWrapperTitle   = document.getElementById('hybridWrapperTitle');

        hybridWrapperTitle.style.top    = ((shim + ((height/4) - (myZoom/20)))-30) + "px";
        hybridWrapperTitle.style.left   =((width/4)+40) + "px";
        hybridWrapperTitle.style.fontSize   = 18 - (myZoom / 20) + "px";

        compileToNativeTitle.style.top  = (shim + ((height/4) - (myZoom/20))) + "px";
        compileToNativeTitle.style.left = ((width/4)+40) + "px";
        compileToNativeTitle.style.fontSize = 18 - (myZoom / 20) + "px";

        nativeTitle.style.top           = (shim + (((height/4) + 26) - (myZoom/20))) + "px";
        nativeTitle.style.left          = ((width/4)+40) + "px";
        nativeTitle.style.fontSize      = 18 - (myZoom / 20) + "px";

        let nativeImages = document.getElementsByClassName('nativeImg');
        for(img of nativeImages){
            img.height = ((height/4)/7)
        }
        let archDiv = document.getElementsByClassName('Arch');
        for(txtDiv of archDiv){
            txtDiv.height = 100;//((height/4)/7)
        }
        let companies = document.getElementsByClassName("Company");
        for(company of companies){
