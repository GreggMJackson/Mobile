        }

        let products = document.getElementsByClassName("Product");
        for(product of products){
            product.height =(height/20);
            product.width  = (width/10);
            //product.style.top  = (height/4) + "px";
            //product.style.left = (100 + ((width/4)/2)) + "px";
        }

        let legends = document.getElementsByClassName("Legend");
        for(legend of legends){
            legend.height = (height/35);
        }

        let opsDeps = document.getElementsByClassName('OpsDeps');
        for(opsDep of opsDeps){
            opsDep.height = (height/35);
        }

        let smallIcons = document.getElementsByClassName('SmallIcons');
        for(smallIcon of smallIcons){
            smallIcon.height = (height/35);
        }

        let languages = document.getElementsByClassName('Language');
        for(language of languages){
            language.height = (height/35);
        }

        let sencha = document.getElementById('Sencha');
        sencha.style.width = (width /5.5) + "px";
        sencha.style.top   = "0px";
        sencha.style.left = (shim + 20) + "px";
        sencha.style.height = ((height/4)-5) + "px";

        let xamarin = document.getElementById('Xamarin');
        xamarin.style.width = (width /5.5) + "px";
        xamarin.style.top   = (height/4) + "px";
        xamarin.style.left = (shim + 20) + "px";
        xamarin.style.height = ((height/4)-5) + "px";

        let natives = document.getElementById('Natives');
        natives.style.top  = ((height/4)+45) + "px";
        natives.style.left = ((width/2)+50) + "px";
        natives.style.borderStyle = "";

        let arches = document.getElementById('Arches');
        arches.style.top  = ((height/4)+45) + "px";
        arches.style.left = ((width/4)+120) + "px";
        arches.style.width = (width/4) + "px";
        arches.style.height = (height/4) + "px";
        arches.style.borderStyle = "";

        let reactNative= document.getElementById('ReactNative');
        reactNative.style.width = (width /5.5) + "px";
        reactNative.style.top   = ((height/4)*2) + "px";
        reactNative.style.left = (shim + 20) + "px";
        reactNative.style.height = ((height/4)-5) + "px";

        let jqueryM = document.getElementById('JQueryM');
        jqueryM.style.width = (width /5.5) + "px";
        jqueryM.style.top   = ((height/4)*3) + "px";
        jqueryM.style.left = (shim + 20) + "px";
        jqueryM.style.height = ((height/4)-5) + "px";

