        framework7.style.width = (width /5.5) + "px";
        framework7.style.top   = "0px";
        framework7.style.left = (shim + ((width/4)*2)) + "px";
        framework7.style.height = ((height/4)-5) + "px";

        let ionic = document.getElementById('Ionic');
        ionic.style.width = (width /5.5) + "px";
        ionic.style.top   = "0px";
        ionic.style.left = (shim + ((width/4)*3)) + "px";
        ionic.style.height = ((height/4)-2) + "px";

        let cordova = document.getElementById('Cordova');
        cordova.style.width = (width /5.5) + "px";
        cordova.style.top   = (height/4) + "px";
        cordova.style.left = (shim + ((width/4)*3)) + "px";
        cordova.style.height = ((height/4)-5) + "px";

        let kendoUI = document.getElementById('KendoUI');
        kendoUI.style.width = (width /5.5) + "px";
        kendoUI.style.top   = ((height/4)*2) + "px";
        kendoUI.style.left = (shim + ((width/4)*3)) + "px";
        kendoUI.style.height = ((height/4)-5) + "px";

        let quasar = document.getElementById('Quasar');
        quasar.style.width = (width /5.5) + "px";
        quasar.style.top   = ((height/4)*3) + "px";
        quasar.style.left = (shim + ((width/4)*2)) + "px";
        quasar.style.height = ((height/4)-5) + "px";

        let onsenUI = document.getElementById('OnsenUI');
        onsenUI.style.width = (width /5.5) + "px";
        onsenUI.style.top   = ((height/4)*3) + "px";
        onsenUI.style.left = (shim + (width/4)) + "px";
        onsenUI.style.height = ((height/4)-5) + "px";

        let titanium = document.getElementById('Titanium');
        titanium.style.width = (width /5.5) + "px";
        titanium.style.top   = ((height/4)*2) + "px";
        titanium.style.left = (shim + (width/4)) + "px";
        titanium.style.height = ((height/4)-5) + "px";

        let nativeScript = document.getElementById('NativeScript');
        nativeScript.style.width = (width /5.5) + "px";
        nativeScript.style.top   = ((height/4)*2) + "px";
        nativeScript.style.left = (shim + ((width/4)*2)) + "px";
        nativeScript.style.height = ((height/4)-5) + "px";
    }
    window.addEventListener('resize', ()=>{ drawFloaters(); });
    onload = ()=>{ drawFloaters(); }
</script>

