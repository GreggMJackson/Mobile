<style>
    .floater {
        position:absolute;
        border-style:solid;
        top:10px;
        left:10px;
        width:50px;
        height:50px;
    }
    img {
        /*border-style:solid;
        border-width:1px;*/
        margin: 2px 0px 2px 2px;
        z-Index: -1;
    }
