        WebView.style.left = (shim + ((width/4)*3) -180) + "px";
        WebView.style.width  = (width/15)  + "px";
        WebView.style.height = (height/40) + "px";
        let WinPhone = document.getElementById('ApacheWinPhoneSDK');
