        let XamarinAndroid = document.getElementById('XamarinAndroid');
        XamarinAndroid.style.top  = (shim + (((height/4) + 78) - (myZoom/20))) + "px";
        XamarinAndroid.style.left = (shim + ((width/4)-26)) + "px";
        XamarinAndroid.style.width  = (width/15)  + "px";
